<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Manifest xmlns="http://schemas.tizen.org/2012/12/manifest">
    <Id>ctLjIIgCCj</Id>
    <Version>0.7.6</Version>
    <Type>C++App</Type>
    <Author>Uladzislau Vasilyeu</Author>
    <Descriptions>
        <Description Locale="eng-GB">MeeCast is a versatile, customizable and yet simple to use and beautiful looking weather forecast application that allows you to set up as much weather stations as you want using various weather forecast providers like Foreca, yr.no, Gismeteo, HKO, Australian Bureau of Meteorology and others to come. Have a quick glance at current conditions along with 7+ day forecast right at the start screen or look into detailed forecast for each day.</Description>
    </Descriptions>
    <Requirements>
        <Feature Name="http://tizen.org/feature/location">true</Feature>
        <Feature Name="http://tizen.org/feature/platform.core.cpu.arch.armv7">true</Feature>
        <Feature Name="http://tizen.org/feature/platform.core.cpu.arch.x86">true</Feature>
        <Feature Name="http://tizen.org/feature/platform.core.fpu.arch.vfpv3">true</Feature>
        <Feature Name="http://tizen.org/feature/screen.size.normal.480.800">true</Feature>
        <Feature Name="http://tizen.org/feature/screen.size.normal.720.1280">true</Feature>
    </Requirements>
    <Apps>
        <ApiVersion>2.2</ApiVersion>
        <Privileges>
            <Privilege>http://tizen.org/privilege/location</Privilege>
            <Privilege>http://tizen.org/privilege/application.launch</Privilege>
            <Privilege>http://tizen.org/privilege/http</Privilege>
        </Privileges>
        <UiApp LaunchingHistoryVisible="True" Main="True" MenuIconVisible="True" Name="meecast">
            <UiScalability BaseScreenSize="Normal" CoordinateSystem="Logical" LogicalCoordinate="720"/>
            <UiTheme SystemTheme="Black"/>
            <DisplayNames>
                <DisplayName Locale="eng-GB">MeeCast</DisplayName>
            </DisplayNames>
            <Icons>
                <Icon Section="MainMenu" Type="Xhigh">mainmenu.png</Icon>
            </Icons>
            <LaunchConditions/>
        </UiApp>
    </Apps>
</Manifest>
